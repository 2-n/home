alt + d
    drun

alt + Return
    alacritty

Print
    shot

alt + Print
    shot -s

alt + q
    waitron window_close

alt + Escape
    $HOME/.config/windowchef/windowchefrc

alt + ctrl + shift + Escape
    waitron wm_quit 0

alt + {_,shift +} r
    winmanip {raise,lower}

alt + {_,shift +} Tab
    winmanip {cycle,revcycle}

alt + {_,shift +} f
    waitron {window_monocle,window_maximize}

alt + {_,shift +} v
    waitron {window_ver_maximize,window_hor_maximize}

alt + {g,y,u,b,n}
    waitron window_snap {middle,topleft,topright,bottomleft,bottomright}

alt + {_,shift +} {h,j,k,l}
    waitron {window_move,window_resize} {-100 0, 0 100, 0 -100, 100 0}

alt + {_,ctrl +,shift +} {1-4}
    waitron {group_activate_specific,group_toggle,group_add_window} {1-4}
